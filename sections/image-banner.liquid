{% schema %}
{
  "name": "t:sections.image-banner.name",
  "tag": "section",
  "class": "slider_section",
  "settings": [
    {
      "type": "image_picker",
      "id": "image",
      "label": "t:sections.image-banner.settings.image.label"
    },
    {
      "type": "image_picker",
      "id": "image_2",
      "label": "t:sections.image-banner.settings.image_2.label"
    },
    {
      "type": "image_picker",
      "id": "image_3",
      "label": "t:sections.image-banner.settings.image_2.label"
    }
  ],
  "blocks": [
    {
      "type": "welcome_message",
      "name": "Welcome message",
      "limit": 1,
      "settings": [
        {
          "type": "text",
          "id": "welcome_message",
          "default": "Welcome To Shree",
          "label": "Welcome message"
        }
      ]
    },
    {
      "type": "shop_name",
      "name": "Shop name",
      "limit": 1,
      "settings": [
        {
          "type": "text",
          "id": "shop_name",
          "default": "Vegetables Shop",
          "label": "Shop name"
        }
      ]
    },
    {
      "type": "shop_description",
      "name": "Shop description",
      "limit": 1,
      "settings": [
        {
          "type": "text",
          "id": "shop_description",
          "default": "It is a long established fact that a reader will be distracted by the readable content of a page when looking at its layout. The point of using Lorem Ipsum is that it has a more-or-less normal",
          "label": "Shop description"
        }
      ]
    }
  ]
  
}
{% endschema %}
